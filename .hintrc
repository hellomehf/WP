{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "backdrop-filter",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "-webkit-print-color-adjust: exact"
        ]
      }
    ]
  }
}